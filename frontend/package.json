{
  "name": "markdowneditor",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "storybook": "export $(cat .env | xargs) && start-storybook -p 7007 -h ${LOCAL_IP-0.0.0.0}",
    "start:storybook": "export $(cat .env | xargs) && LOAD_STORYBOOK=true expo start",
    "build-storybook": "build-storybook",
    "web": "expo start --web",
    "build:web": "expo build:web",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "test": "jest --watchAll --coverage",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/uuid": "^8.3.4",
    "expo": "~44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-font": "~10.0.4",
    "expo-status-bar": "~1.2.0",
    "expo-web-browser": "~10.1.0",
    "jsonwebtoken": "^8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-get-random-values": "^1.8.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-native-web-hooks": "^3.0.2",
    "react-redux": "^8.0.2",
    "socket.io-client": "^4.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-storyshots": "^6.5.9",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "17",
    "babel-loader": "^8.2.5",
    "expo-cli": "^5.4.3",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17",
    "typescript": "~4.3.5"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/assets/"
    ]
  }
}
